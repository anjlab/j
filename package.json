{
  "name": "jj-flow",
  "version": "2.0.1",
  "description": "FlowTyped helpers for working with json",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build:lib": "babel --out-dir lib src",
    "prebuild:lib": "rm -rf lib",
    "postbuild:lib": "flow gen-flow-files src/index.js > lib/index.js.flow"
  },
  "repository": {},
  "author": "Yury Korolev",
  "license": "MIT",
  "peerDependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.16.3",
    "flow-bin": "^0.36.0",
    "flow-typed": "^2.0.0",
    "jest": "^18.1.0"
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-runtime",
      "transform-flow-strip-types"
    ]
  }
}
